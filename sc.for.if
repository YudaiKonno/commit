import java.util.*;


public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        int times =sc.nextInt(); 
        int sum = 0;
        
        for (int i = 0;i < times;i++ ){
        int num = sc.nextInt();
            if (num >= 5){
                sum += num; 
            } 

            } 
        System.out.println(sum);
        sc.close();
        } 
        
    }
