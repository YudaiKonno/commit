import java.util.*;


public class Main {
    public static void main(String[] args) {
        int[] array = {4, 0, 5, -1, 3, 10, 6, -8};
        int num = 0; 
            for (int i = 0; i < array.length;i++ ){
               if (array[i] >= 5){
                num += array[i];
                   
               } 
            } 
        System.out.println(num);
    }
}
