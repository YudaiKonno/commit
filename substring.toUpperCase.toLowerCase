import java.util.*;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！

        Scanner sc = new Scanner(System.in);
        int start = sc.nextInt();
        int end = sc.nextInt();
        sc.nextLine();
        String str = sc.nextLine();
        
        System.out.println(str.substring(0,start -1) + str.substring(start -1,end).toUpperCase() + str.substring(end));
    }
}
