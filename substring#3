import java.util.*;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！

        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        sc.nextLine();
        String s[] = sc.nextLine().split("");
            if (n < s.length){
                
        System.out.println(s[n - 1]+" "+s[n]);
            } 
    }
}
