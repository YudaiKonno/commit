import java.util.*;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！

        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        
        String[] array = {" ","6B","5B","4B","3B","2B","B","HB","F","H","2H","3H","4H","5H","6H","7H","8H","9H"};
        System.out.println(array[num]);
    }
}
