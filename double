import java.util.*;


public class Main {
    public static void main(String[] args) {
        // 自分の得意な言語で
        // Let's チャレンジ！！

        Scanner sc = new Scanner(System.in);
        double a = sc.nextDouble();
        
        
        System.out.print((a - 18.0));
        System.out.print(" ");
        System.out.print((a - 18.5));
    }
}
